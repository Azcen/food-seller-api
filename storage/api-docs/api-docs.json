                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string",
                        "format": "password"
                    }
                },
                "type": "object"
            },
            "LoginResponseSchema": {
                "title": "Login Response",
                "properties": {
                    "user": {
                        "properties": {
                            "id": {
                                "type": "integer",
                                "example": "1"
                            },
                            "name": {
                                "type": "string",
                                "example": "Alex Ochoa"
                            },
                            "email": {
                                "type": "string",
                                "example": "alexx1708@gmail.com"
                            },
